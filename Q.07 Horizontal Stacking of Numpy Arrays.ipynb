   "source": [
    "### Q.7 Horizontal Stacking of Numpy Arrays"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "40e2828b",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "d3f0b9dd",
   "metadata": {},
   "outputs": [],
   "source": [
    "arr1 = np.array([[1,2,3], [7,8,9]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "f988f67d",
   "metadata": {},
   "outputs": [],
   "source": [
    "arr2 = np.array([[4,5,6], [10,11,12]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "cb6ba056",
   "metadata": {},
   "outputs": [],
   "source": [
    "final_arr = np.hstack((arr1, arr2))\n"
   ]
  },
  {
